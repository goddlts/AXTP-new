import request from '@/utils/request'

export function getList (params) {
  return request({
    url: '/{{name}}',
    method: 'get',
    params
  })
}

export function remove{{upperCase name}} (id) {
  return request({
    url: `/{{name}}/${id}`,
    method: 'delete'
  })
}

export function add{{upperCase name}} (data) {
  return request({
    url: `/{{name}}`,
    method: 'post',
    data
  })
}

export function edit{{upperCase name}} (id, data) {
  return request({
    url: `/{{name}}/${id}`,
    method: 'put',
    data
  })
}

export function get{{upperCase name}}ById (id) {
  return request({
    url: `/{{name}}/${id}`,
    method: 'get'
  })
}
